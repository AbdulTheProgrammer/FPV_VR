<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.constantin.wilson.FPV_VR">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.constantin.wilson.FPV_VR.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.constantin.wilson.FPV_VR.OpenGLActivity"
            android:label="@string/title_activity_open_gl"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name="com.constantin.wilson.FPV_VR.SurfaceViewActivity"
            android:label="@string/title_activity_surface_view"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name="com.constantin.wilson.FPV_VR.TextureViewActivity"
            android:label="@string/title_activity_my_texture_view"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name="com.constantin.wilson.FPV_VR.SettingsActivity"
            android:label="@string/title_activity_settings"
            android:theme="@style/AppTheme.NoActionBar" />
        <!--
             ATTENTION: This was auto-generated to add Google Play services to your project for
             App Indexing.  See https://g.co/AppIndexing/AndroidStudio for more information.
        -->
        <meta-data
            android:name="com.google.android.gms.version"
            android:value="@integer/google_play_services_version" />

        <activity android:name="com.constantin.wilson.FPV_VR.TestActivity"></activity>
    </application>

</manifest>
